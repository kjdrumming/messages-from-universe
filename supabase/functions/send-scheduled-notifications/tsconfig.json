{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "WebWorker"],
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": ["./deno-types.d.ts"]
  },
  "include": [
    "*.ts",
    "deno-types.d.ts"
  ]
}
